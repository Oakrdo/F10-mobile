-- Debug Explorer para encontrar Tennenman - Dragon Ball Rage
-- Por: DeepSeek-R1

wait(2)

local Players = game:GetService("Players")
local Workspace = game:GetService("Workspace")
local player = Players.LocalPlayer

-- GUI de Debug
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TennenmanDebug"
screenGui.Parent = player:WaitForChild("PlayerGui")

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0.8, 0, 0.8, 0)
mainFrame.Position = UDim2.new(0.1, 0, 0.1, 0)
mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
mainFrame.BackgroundTransparency = 0.1
mainFrame.Parent = screenGui

local title = Instance.new("TextLabel")
title.Text = "üîç DEBUG TENNENMAN - DRAGON BALL RAGE"
title.Size = UDim2.new(1, 0, 0.1, 0)
title.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
title.TextColor3 = Color3.new(1, 1, 1)
title.TextScaled = true
title.Font = Enum.Font.SourceSansBold
title.Parent = mainFrame

local scrollFrame = Instance.new("ScrollingFrame")
scrollFrame.Size = UDim2.new(1, 0, 0.9, 0)
scrollFrame.Position = UDim2.new(0, 0, 0.1, 0)
scrollFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 20)
scrollFrame.ScrollBarThickness = 10
scrollFrame.Parent = mainFrame

local contentFrame = Instance.new("Frame")
contentFrame.Size = UDim2.new(1, 0, 0, 0)
contentFrame.BackgroundTransparency = 1
contentFrame.Parent = scrollFrame

-- Fun√ß√£o para adicionar item na lista
local function addDebugItem(text, color, isHeader)
    local itemCount = #contentFrame:GetChildren()
    local itemFrame = Instance.new("Frame")
    itemFrame.Size = UDim2.new(1, 0, 0, 30)
    itemFrame.Position = UDim2.new(0, 0, 0, itemCount * 30)
    itemFrame.BackgroundColor3 = itemCount % 2 == 0 and Color3.fromRGB(30, 30, 40) or Color3.fromRGB(25, 25, 35)
    itemFrame.BorderSizePixel = 0
    itemFrame.Parent = contentFrame
    
    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1, -10, 1, 0)
    label.Position = UDim2.new(0, 5, 0, 0)
    label.Text = text
    label.TextColor3 = color or Color3.new(1, 1, 1)
    label.TextScaled = true
    label.TextXAlignment = Enum.TextXAlignment.Left
    label.BackgroundTransparency = 1
    label.Parent = itemFrame
    
    if isHeader then
        itemFrame.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
        label.Font = Enum.Font.SourceSansBold
    end
    
    contentFrame.Size = UDim2.new(1, 0, 0, (itemCount + 1) * 30)
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, (itemCount + 1) * 30)
end

-- Fun√ß√£o para explorar toda a estrutura do Workspace
local function exploreWorkspace()
    addDebugItem("=== EXPLORANDO WORKSPACE ===", Color3.new(1, 0.5, 0), true)
    
    -- Listar todas as pastas principais
    for _, child in pairs(Workspace:GetChildren()) do
        addDebugItem("üìÅ " .. child.Name .. " (" .. child.ClassName .. ")", Color3.new(0.8, 0.8, 1))
        
        -- Explorar filhos das pastas importantes
        if child:IsA("Folder") or child:IsA("Model") then
            for _, grandChild in pairs(child:GetChildren()) do
                addDebugItem("   ‚îú‚îÄ‚îÄ " .. grandChild.Name .. " (" .. grandChild.ClassName .. ")", Color3.new(0.6, 0.8, 1))
                
                -- Procurar por NPCs, Tennenman, ou nomes similares
                if string.find(grandChild.Name:lower(), "npc") or 
                   string.find(grandChild.Name:lower(), "tennen") or
                   string.find(grandChild.Name:lower(), "enemy") then
                    addDebugItem("   ‚îÇ   üéØ POSS√çVEL NPC: " .. grandChild.Name, Color3.new(1, 1, 0))
                    
                    -- Explorar mais fundo
                    for _, greatGrandChild in pairs(grandChild:GetChildren()) do
                        addDebugItem("   ‚îÇ   ‚îú‚îÄ‚îÄ " .. greatGrandChild.Name .. " (" .. greatGrandChild.ClassName .. ")", Color3.new(0.8, 1, 0.8))
                    end
                end
            end
        end
    end
    
    addDebugItem("", Color3.new(1, 1, 1))
    addDebugItem("=== PROCURANDO POR TENNENMAN ESPECIFICAMENTE ===", Color3.new(1, 0.5, 0), true)
    
    -- Buscar por "Tennenman" em todo o Workspace
    local tennenmanCount = 0
    for _, descendant in pairs(Workspace:GetDescendants()) do
        if string.find(descendant.Name:lower(), "tennen") then
            tennenmanCount = tennenmanCount + 1
            local path = getFullPath(descendant)
            addDebugItem("üéØ TENNENMAN ENCONTRADO: " .. path, Color3.new(0, 1, 0))
        end
    end
    
    if tennenmanCount == 0 then
        addDebugItem("‚ùå NENHUM TENNENMAN ENCONTRADO NO WORKSPACE", Color3.new(1, 0, 0))
        
        -- Procurar por nomes alternativos
        addDebugItem("", Color3.new(1, 1, 1))
        addDebugItem("=== PROCURANDO POR NOMES ALTERNATIVOS ===", Color3.new(1, 0.5, 0), true)
        
        local alternativeNames = {"boss", "enemy", "npc", "mob", "villain", "opponent"}
        for _, name in pairs(alternativeNames) do
            local count = 0
            for _, descendant in pairs(Workspace:GetDescendants()) do
                if string.find(descendant.Name:lower(), name) and descendant:IsA("Model") then
                    count = count + 1
                    if count <= 5 then -- Limitar para n√£o sobrecarregar
                        addDebugItem("ü§î " .. name:upper() .. ": " .. getFullPath(descendant), Color3.new(1, 0.8, 0))
                    end
                end
            end
            if count > 0 then
                addDebugItem("üìä Total de " .. name .. ": " .. count, Color3.new(0.8, 0.8, 1))
            end
        end
    end
    
    addDebugItem("", Color3.new(1, 1, 1))
    addDebugItem("=== MODELOS COM HUMANOID (POSS√çVEIS INIMIGOS) ===", Color3.new(1, 0.5, 0), true)
    
    -- Listar todos os modelos com Humanoid (poss√≠veis inimigos)
    local humanoidCount = 0
    for _, descendant in pairs(Workspace:GetDescendants()) do
        if descendant:IsA("Model") and descendant:FindFirstChildOfClass("Humanoid") then
            humanoidCount = humanoidCount + 1
            if humanoidCount <= 10 then -- Limitar para n√£o sobrecarregar
                local humanoid = descendant:FindFirstChildOfClass("Humanoid")
                addDebugItem("üë§ " .. descendant.Name .. " (Health: " .. humanoid.Health .. ")", Color3.new(1, 1, 0.5))
            end
        end
    end
    addDebugItem("üìä Total de modelos com Humanoid: " .. humanoidCount, Color3.new(0.8, 0.8, 1))
end

-- Fun√ß√£o para obter o caminho completo de um objeto
local function getFullPath(obj)
    local path = obj.Name
    local parent = obj.Parent
    while parent and parent ~= Workspace do
        path = parent.Name .. " ‚Üí " .. path
        parent = parent.Parent
    end
    return "Workspace ‚Üí " .. path
end

-- Bot√£o para atualizar
local refreshButton = Instance.new("TextButton")
refreshButton.Text = "üîÑ ATUALIZAR EXPLORA√á√ÉO"
refreshButton.Size = UDim2.new(0.4, 0, 0.08, 0)
refreshButton.Position = UDim2.new(0.3, 0, 0.92, 0)
refreshButton.BackgroundColor3 = Color3.fromRGB(80, 160, 220)
refreshButton.TextColor3 = Color3.new(1, 1, 1)
refreshButton.TextScaled = true
refreshButton.Parent = screenGui

refreshButton.MouseButton1Click:Connect(function()
    -- Limpar conte√∫do anterior
    for _, child in pairs(contentFrame:GetChildren()) do
        child:Destroy()
    end
    contentFrame.Size = UDim2.new(1, 0, 0, 0)
    
    -- Nova explora√ß√£o
    exploreWorkspace()
end)

-- Iniciar explora√ß√£o
exploreWorkspace()

print("üîç Debug Explorer carregado!")
print("Verifique a tela para ver a estrutura completa do jogo")
print("Use o bot√£o üîÑ para atualizar a explora√ß√£o")